	<xs:complexType name="tableau">
		<xs:choice maxOccurs="unbounded">
			<xs:element name="entete">
				<xs:annotation>
					<xs:documentation>Ligne entête de tableau</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence maxOccurs="unbounded">
						<xs:element name="colonne" type="colonne">
							<xs:annotation>
								<xs:documentation>Cellule de tableau</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ligne">
				<xs:annotation>
					<xs:documentation>Ligne normale de tableau</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence maxOccurs="unbounded">
						<xs:element name="colonne" type="colonne">
							<xs:annotation>
								<xs:documentation>Cellule de tableau</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
		<xs:attribute name="width" type="largeurRelative"/>
		<xs:attribute name="border" type="xs:integer"/>
		<xs:attribute name="sautDePagePdf" type="zeroUn" default="0"/>
		<xs:attribute name="legende" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="colonne" mixed="true">
		<xs:complexContent>
			<xs:extension base="zoneRedigeeMixte">
				<xs:attribute name="colspan" type="xs:integer"/>
				<xs:attribute name="rowspan" type="xs:integer"/>
				<xs:attribute name="useText" type="zeroUn" default="0"/>
				<xs:attribute name="align" type="alignement"/>
				<xs:attribute name="width" type="largeurRelative"/>
				<xs:attribute name="color" type="xs:string"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="largeurRelative">
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{1,3}(%|px)?"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="animation">
		<xs:all>
			<xs:element name="width" type="xs:integer">
				<xs:annotation>
					<xs:documentation>Largeur de la vidéo</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="height" type="xs:integer">
				<xs:annotation>
					<xs:documentation>Hauteur de la vidéo</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="title" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Titre de la vidéo</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="config" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Lien d'un fichier .ini qui contient la configuration que l'on souhaite pour notre lecteur mp3</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="image" type="image" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Image qui sera cliquable pour lancer la vidéo</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- Balise codebase obselete -->
			<xs:element name="param-movie" type="xs:string">
				<xs:annotation>
					<xs:documentation>URL complète de l'animation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="param-quality" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Qualité de l'animation</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="low"/>
						<xs:enumeration value="medium"/>
						<xs:enumeration value="high"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="param-loop" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Déterminer si l'animation doit boucler (true ou false)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="true"/>
						<xs:enumeration value="false"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="param-wmode" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Déterminer le fond de l'animation, couleur correspondant à une couleur HTML (#FFFFFF...)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
		<xs:attribute name="type" >
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="dailymotion"/>
					<xs:enumeration value="flash"/>
					<xs:enumeration value="flv"/>
					<xs:enumeration value="m4v"/>
					<xs:enumeration value="mp3"/>
					<xs:enumeration value="mp4"/>
					<xs:enumeration value="ogg"/>
					<xs:enumeration value="oggaudio"/>
					<xs:enumeration value="swf"/>
					<xs:enumeration value="slideshare"/>
					<xs:enumeration value="speakerdeck"/>
					<xs:enumeration value="vimeo"/>
					<xs:enumeration value="wav"/>
					<xs:enumeration value="webm"/>
					<xs:enumeration value="youtube"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="imgtext" mixed="true">
