   <db:section xml:id="sha-1-sha1-cpp">
      <db:title>SHA-1 (sha1.cpp)</db:title>
      <db:para>
         <db:emphasis>Based on the public domain implementation of the SHA-1
                    algorithm</db:emphasis>
         <db:emphasis>Copyright (C) Dominik Reichl
                  &lt;dominik.reichl@t-online.de&gt;</db:emphasis>
      </db:para>
