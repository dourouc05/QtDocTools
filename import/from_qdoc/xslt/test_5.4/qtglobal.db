                                    <db:code><db:link xlink:href="qtglobal.db#Q_OS_WIN64">Q_OS_WIN64</db:link></db:code> or
                                    <db:code><db:link xlink:href="qtglobal.db#Q_OS_WINCE">Q_OS_WINCE</db:link></db:code> is
                                defined.</db:para>
      </db:section>
