               <db:td>
                  <db:para>qreal</db:para>
               </db:td>
            </db:tr>
            <db:tr>
               <db:td>
                  <db:para>
                     <db:link xlink:href="qmediametadata.db#DateTimeOriginal-var">DateTimeOriginal</db:link>
                  </db:para>
               </db:td>
               <db:td>
                  <db:para>The date and time when the original image
                                                  data was generated.</db:para>
               </db:td>
               <db:td>
                  <db:para>
                     <db:code><db:link xlink:href="qdatetime.db">QDateTime</db:link></db:code>
                  </db:para>
               </db:td>
            </db:tr>
            <db:tr>
               <db:td>
                  <db:para>
                     <db:link xlink:href="qmediametadata.db#DateTimeDigitized-var">DateTimeDigitized</db:link>
                  </db:para>
               </db:td>
               <db:td>
                  <db:para>The date and time when the image was stored
                                                  as digital data.</db:para>
               </db:td>
               <db:td>
                  <db:para>
                     <db:code><db:link xlink:href="qdatetime.db">QDateTime</db:link></db:code>
                  </db:para>
               </db:td>
            </db:tr>
            <db:tr>
               <db:td>
                  <db:para>
                     <db:link xlink:href="qmediametadata.db#SubjectDistance-var">SubjectDistance</db:link>
                  </db:para>
               </db:td>
               <db:td>
                  <db:para>The distance to the subject, given in
                                                  meters.</db:para>
               </db:td>
               <db:td>
                  <db:para>qreal</db:para>
               </db:td>
            </db:tr>
            <db:tr>
               <db:td>
                  <db:para>
                     <db:link xlink:href="qmediametadata.db#MeteringMode-var">MeteringMode</db:link>
