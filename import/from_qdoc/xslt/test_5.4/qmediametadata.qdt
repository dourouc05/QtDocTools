                     <db:code><db:link xlink:href="qcameraimageprocessing.db#WhiteBalanceMode-enum">QCameraImageProcessing::WhiteBalanceMode</db:link></db:code>
                  </db:para>
               </db:td>
            </db:tr>
            <db:tr>
               <db:td>
                  <db:para>
                     <db:link xlink:href="qmediametadata.db#DigitalZoomRatio-var">DigitalZoomRatio</db:link>
