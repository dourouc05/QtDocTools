                  <db:para>
                     <db:code><db:link xlink:href="qstring.db">QString</db:link></db:code>
                  </db:para>
               </db:td>
            </db:tr>
            <db:tr>
               <db:td>
                  <db:para>
                     <db:link xlink:href="qmediametadata.db#CameraModel-var">CameraModel</db:link>
