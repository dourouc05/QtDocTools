               </db:member>
               <db:member>
                  <db:code><db:link xlink:href="qtgui/qcloseevent.html">QCloseEvent</db:link></db:code>
               </db:member>
               <db:member>
                  <db:link xlink:href="qtwidgets-mainwindows-application-example.html">Application Example</db:link>
               </db:member>
            </db:simplelist>
