               </db:member>
               <db:member>
                  <db:code><db:link xlink:href="qtgui/qkeyevent.html">QKeyEvent</db:link></db:code>
               </db:member>
               <db:member>
                  <db:link xlink:href="qtwidgets-widgets-tetrix-example.html">Tetrix Example</db:link>
               </db:member>
            </db:simplelist>
         </db:section>
      </db:section>
