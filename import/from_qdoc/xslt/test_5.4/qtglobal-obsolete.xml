<!-- @@@QMAX -->
<!-- $$$QMIN[overload1]$$$QMIN -->
<html:h3 class="fn" id="QMIN"><html:a name="QMIN" /><html:span class="name">QMIN</html:span>(<html:i> x</html:i>, <html:i> y</html:i>)</html:h3>
