   <db:title>DwmFeatures QML Type</db:title>
   <db:classsynopsis xlink:href="qml-qtwinextras-dwmfeatures.db">
      <db:ooclass>
         <db:classname>DwmFeatures</db:classname>
      </db:ooclass>
      <db:classsynopsisinfo role="import">import QtWinExtras 1.0</db:classsynopsisinfo>
      <db:classsynopsisinfo role="since"> QtWinExtras
                                                1.0</db:classsynopsisinfo>
      <db:fieldsynopsis xlink:href="#blurBehindEnabled-prop">
         <db:type>bool</db:type>
         <db:varname>blurBehindEnabled</db:varname>
      </db:fieldsynopsis>
      <db:fieldsynopsis xlink:href="#bottomGlassMargin-prop">
         <db:type>int</db:type>
         <db:varname>bottomGlassMargin</db:varname>
      </db:fieldsynopsis>
      <db:fieldsynopsis xlink:href="#excludedFromPeek-prop">
         <db:type>bool</db:type>
         <db:varname>excludedFromPeek</db:varname>
      </db:fieldsynopsis>
      <db:fieldsynopsis xlink:href="#flip3DPolicy-prop">
         <db:type xlink:href="qtwin.html#WindowFlip3DPolicy-enum">QtWin::WindowFlip3DPolicy</db:type>
         <db:varname>flip3DPolicy</db:varname>
      </db:fieldsynopsis>
      <db:fieldsynopsis xlink:href="#leftGlassMargin-prop">
         <db:type>int</db:type>
         <db:varname>leftGlassMargin</db:varname>
      </db:fieldsynopsis>
      <db:fieldsynopsis xlink:href="#peekDisallowed-prop">
         <db:type>bool</db:type>
         <db:varname>peekDisallowed</db:varname>
      </db:fieldsynopsis>
