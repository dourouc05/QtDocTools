               <db:member><db:code><db:link xlink:href="qwidget.html#resizeEvent">resizeEvent</db:link>()</db:code></db:member>
               <db:member><db:code><db:link xlink:href="qtcore/qobject.html#event">QObject::event</db:link>()</db:code></db:member>
