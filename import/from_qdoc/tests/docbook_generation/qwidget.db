               <db:member><db:code><db:link xlink:href="qwidget.html#keyReleaseEvent">keyReleaseEvent</db:link>()</db:code></db:member>
               <db:member><db:code><db:link xlink:href="qwidget.html#event">event</db:link>()</db:code></db:member>
               <db:member><db:code><db:link xlink:href="qtgui/qfocusevent.html">QFocusEvent</db:link></db:code></db:member>
            </db:simplelist>
         </db:section>
      </db:section>
