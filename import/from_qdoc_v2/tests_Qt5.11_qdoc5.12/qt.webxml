            <enum threadsafety="unspecified" name="ConnectionType" fullname="Qt::ConnectionType" href="qt.html#ConnectionType-enum" status="active" access="public" location="qnamespace.h" filepath="C:/Qt/5.11.1/msvc2017_64/include\QtCore/qnamespace.h" lineno="1289">
