         <db:varlistentry>
            <db:term>Material.Red</db:term>
            <db:listitem>
               <db:programlisting role="raw-html">&lt;div style="padding:10px;color:#fff;background:#F44336;"&gt;#F44336 &lt;/div&gt;</db:programlisting>
