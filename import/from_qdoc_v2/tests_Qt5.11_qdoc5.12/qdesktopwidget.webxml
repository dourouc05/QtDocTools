        <class threadsafety="unspecified" name="QDesktopWidget" href="qdesktopwidget.html" status="obsolete" access="public" location="kernel\qdesktopwidget.h" filepath="C:/Qt/5.11.1/Src/qtbase/src/widgets/kernel\qdesktopwidget.h" lineno="52" bases="QWidget" module="QtWidgets" groups="advanced,desktop" brief="Access to screen information on multi-head systems">
