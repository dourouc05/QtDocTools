                            <term>Material.Yellow</term>Material.Yellow</definition>
                        <item>
                            <raw format="HTML">&lt;div style=&quot;padding:10px;color:#fff;background:#FFF59D;&quot;&gt;#FFF59D &lt;/div&gt;</raw>
                        </item>
                        <definition>
                            <term>Material.Amber</term>Material.Amber</definition>
                        <item>
                            <raw format="HTML">&lt;div style=&quot;padding:10px;color:#fff;background:#FFE082;&quot;&gt;#FFE082 &lt;/div&gt;</raw>
                        </item>
                        <definition>
                            <term>Material.Orange</term>Material.Orange</definition>
                        <item>
                            <raw format="HTML">&lt;div style=&quot;padding:10px;color:#fff;background:#FFCC80;&quot;&gt;#FFCC80 &lt;/div&gt;</raw>
                        </item>
