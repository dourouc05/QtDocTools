   <db:info>
      <db:title>Miscellaneous</db:title>
      <db:abstract>
         <db:para/>
         <db:para>
            <db:simplelist>
               <db:member>
                  <db:link xlink:href="21-0-qdoc-creating-dita-maps.html" xlink:title="next">Creating DITA Maps &gt;</db:link>
               </db:member>
