                  <db:td>Qt::TextEditorInteraction</db:td>
                  <db:td>TextSelectableByMouse | TextSelectableByKeyboard | TextEditable</db:td>
                  <db:td>
                     <db:para>The default for a text editor.</db:para>
                  </db:td>
               </db:tr>
               <db:tr>
