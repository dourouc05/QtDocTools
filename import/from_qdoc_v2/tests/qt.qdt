            <db:enumitem>
               <db:enumidentifier>ImPreferredLanguage</db:enumidentifier>
               <db:enumvalue>0x200</db:enumvalue>
            </db:enumitem>
            <db:enumitem>
               <db:enumidentifier>ImAbsolutePosition</db:enumidentifier>
               <db:enumvalue>0x400</db:enumvalue>
            </db:enumitem>
            <db:enumitem>
               <db:enumidentifier>ImTextBeforeCursor</db:enumidentifier>
               <db:enumvalue>0x800</db:enumvalue>
            </db:enumitem>
            <db:enumitem>
               <db:enumidentifier>ImTextAfterCursor</db:enumidentifier>
               <db:enumvalue>0x1000</db:enumvalue>
            </db:enumitem>
            <db:enumitem>
               <db:enumidentifier>ImEnterKeyType</db:enumidentifier>
               <db:enumvalue>0x2000</db:enumvalue>
            </db:enumitem>
            <db:enumitem>
               <db:enumidentifier>ImAnchorRectangle</db:enumidentifier>
               <db:enumvalue>0x4000</db:enumvalue>
            </db:enumitem>
            <db:enumitem>
               <db:enumidentifier>ImInputItemClipRectangle</db:enumidentifier>
               <db:enumvalue>0x8000</db:enumvalue>
            </db:enumitem>
            <db:enumitem>
               <db:enumidentifier>ImPlatformData</db:enumidentifier>
