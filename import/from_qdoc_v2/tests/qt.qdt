                  </db:th>
                  <db:th>
                     <db:para>Value</db:para>
                  </db:th>
                  <db:th>
                     <db:para>Description</db:para>
                  </db:th>
               </db:tr>
            </db:thead>
            <db:tbody>
               <db:tr>
                  <db:td>Qt::MouseFocusReason</db:td>
                  <db:td>0</db:td>
                  <db:td>
                     <db:para>A mouse action occurred.</db:para>
                  </db:td>
               </db:tr>
               <db:tr>
                  <db:td>Qt::TabFocusReason</db:td>
