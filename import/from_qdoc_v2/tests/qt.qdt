            </db:enumitem>
            <db:enumitem>
               <db:enumidentifier>GroupSwitchModifier</db:enumidentifier>
               <db:enumvalue>0x40000000</db:enumvalue>
            </db:enumitem>
            <db:enumitem>
               <db:enumidentifier>KeyboardModifierMask</db:enumidentifier>
