                  <db:td>
                     <db:para>The application opened/closed a pop-up that grabbed/released the keyboard focus.</db:para>
                  </db:td>
               </db:tr>
               <db:tr>
                  <db:td>Qt::ShortcutFocusReason</db:td>
                  <db:td>5</db:td>
                  <db:td>
