                  </db:th>
                  <db:th>
                     <db:para>Value</db:para>
                  </db:th>
                  <db:th>
                     <db:para>Description</db:para>
                  </db:th>
               </db:tr>
            </db:thead>
            <db:tbody>
               <db:tr>
                  <db:td>Qt::ReplaceSelection</db:td>
                  <db:td>0</db:td>
                  <db:td>
                     <db:para>The currently selected items are replaced by items in the selection area.</db:para>
                  </db:td>
               </db:tr>
               <db:tr>
                  <db:td>Qt::AddToSelection</db:td>
                  <db:td>1</db:td>
                  <db:td>
                     <db:para>The items in the selection area are added to the currently selected items.</db:para>
                  </db:td>
               </db:tr>
            </db:tbody>
         </db:informaltable>
