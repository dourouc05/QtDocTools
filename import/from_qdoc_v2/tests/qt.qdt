            </db:enumitem>
            <db:enumitem>
               <db:enumidentifier>ImQueryInput</db:enumidentifier>
               <db:enumvalue>ImCursorRectangle | ImCursorPosition | ImSurroundingText |&#xD;
                       ImCurrentSelection | ImAnchorRectangle | ImAnchorPosition</db:enumvalue>
