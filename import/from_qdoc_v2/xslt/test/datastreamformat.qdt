                  <db:para>unsigned 16-bit integer</db:para>
               </db:listitem>
            </db:itemizedlist>
         </db:td>
      </db:tr>
      <db:tr>
         <db:td>
            <db:para>quint32</db:para>
         </db:td>
         <db:td>
