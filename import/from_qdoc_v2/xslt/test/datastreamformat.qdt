               <db:listitem>
                  <db:para>Blue value (quint16)</db:para>
               </db:listitem>
               <db:listitem>
                  <db:para>Pad value (quint16)</db:para>
               </db:listitem>
            </db:itemizedlist>
         </db:td>
      </db:tr>
      <db:tr>
         <db:td>
