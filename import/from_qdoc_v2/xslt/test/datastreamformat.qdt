         </db:td>
         <db:td>
            <db:itemizedlist>
               <db:listitem>
                  <db:para>unsigned 64-bit integer</db:para>
               </db:listitem>
            </db:itemizedlist>
         </db:td>
      </db:tr>
      <db:tr>
         <db:td>
            <db:para>
