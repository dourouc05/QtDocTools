        <page name="basic" href="qtvirtualkeyboard-basic-example.html" status="active" location="qtvirtualkeyboard-examples.qdoc" filepath="C:/Qt/5.11.1/Src/qtvirtualkeyboard/src/virtualkeyboard/doc/src/qtvirtualkeyboard-examples.qdoc" lineno="34" subtype="example" title="Qt Quick Virtual Keyboard - Basic Example" fulltitle="Qt Quick Virtual Keyboard - Basic Example" subtitle="" module="QtVirtualKeyboard" groups="qtvirtualkeyboard-examples" brief="This is a basic QML example that uses the virtual keyboard">
