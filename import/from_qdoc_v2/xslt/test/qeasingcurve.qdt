            <db:code><db:link xlink:href="qeasingcurve.html#Type-enum" xrefstyle="enum" annotations="QEasingCurve::InBounce">QEasingCurve::InBounce</db:link></db:code>, <db:code><db:link xlink:href="qeasingcurve.html#Type-enum" xrefstyle="enum" annotations="QEasingCurve::OutBounce">QEasingCurve::OutBounce</db:link></db:code>, <db:code><db:link xlink:href="qeasingcurve.html#Type-enum" xrefstyle="enum" annotations="QEasingCurve::InOutBounce">QEasingCurve::InOutBounce</db:link></db:code>, <db:code><db:link xlink:href="qeasingcurve.html#Type-enum" xrefstyle="enum" annotations="QEasingCurve::OutInBounce">QEasingCurve::OutInBounce</db:link></db:code>, <db:code><db:link xlink:href="qeasingcurve.html#Type-enum" xrefstyle="enum" annotations="QEasingCurve::InElastic">QEasingCurve::InElastic</db:link></db:code>, <db:code><db:link xlink:href="qeasingcurve.html#Type-enum" xrefstyle="enum" annotations="QEasingCurve::OutElastic">QEasingCurve::OutElastic</db:link></db:code>, <db:code><db:link xlink:href="qeasingcurve.html#Type-enum" xrefstyle="enum" annotations="QEasingCurve::InOutElastic">QEasingCurve::InOutElastic</db:link></db:code> or <db:code><db:link xlink:href="qeasingcurve.html#Type-enum" xrefstyle="enum" annotations="QEasingCurve::OutInElastic">QEasingCurve::OutInElastic</db:link></db:code>).</db:para>
