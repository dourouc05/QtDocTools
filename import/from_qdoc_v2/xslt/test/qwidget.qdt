      <db:section>
         <db:title>void QWidget::setContentsMargins(const QMargins &amp;margins)</db:title>
         <db:methodsynopsis>
            <db:void/>
            <db:methodname>setContentsMargins</db:methodname>
            <db:methodparam>
               <db:type>const QMargins &amp;</db:type>
               <db:parameter>margins</db:parameter>
