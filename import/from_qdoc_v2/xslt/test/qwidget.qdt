               </db:member>
               <db:member>
                  <db:code><db:link xlink:href="qwidget.html#minimumSize-prop" xrefstyle="property" annotations="minimumSize">minimumSize</db:link></db:code>
               </db:member>
               <db:member>
                  <db:code><db:link xlink:href="qwidget.html#maximumSize-prop" xrefstyle="property" annotations="maximumSize">maximumSize</db:link></db:code>
               </db:member>
               <db:member>
                  <db:code><db:link xlink:href="qwidget.html#resizeEvent" xrefstyle="function" annotations="resizeEvent()">resizeEvent()</db:link></db:code>
               </db:member>
               <db:member>
