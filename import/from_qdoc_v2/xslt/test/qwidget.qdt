               </db:member>
               <db:member>
                  <db:code><db:link xlink:href="qwidget.html#rect-prop" xrefstyle="function" annotations="rect()">rect()</db:link></db:code>
               </db:member>
               <db:member>
                  <db:code><db:link xlink:href="qwidget.html#pos-prop" xrefstyle="function" annotations="move()">move()</db:link></db:code>
               </db:member>
               <db:member>
                  <db:code><db:link xlink:href="qwidget.html#size-prop" xrefstyle="function" annotations="resize()">resize()</db:link></db:code>
               </db:member>
               <db:member>
                  <db:code><db:link xlink:href="qwidget.html#moveEvent" xrefstyle="function" annotations="moveEvent()">moveEvent()</db:link></db:code>
               </db:member>
               <db:member>
                  <db:code><db:link xlink:href="qwidget.html#resizeEvent" xrefstyle="function" annotations="resizeEvent()">resizeEvent()</db:link></db:code>
               </db:member>
               <db:member>
                  <db:code><db:link xlink:href="qwidget.html#minimumSize-prop" xrefstyle="function" annotations="minimumSize()">minimumSize()</db:link></db:code>
               </db:member>
               <db:member>
                  <db:code><db:link xlink:href="qwidget.html#maximumSize-prop" xrefstyle="function" annotations="maximumSize()">maximumSize()</db:link></db:code>
               </db:member>
            </db:simplelist>
         </db:para>
