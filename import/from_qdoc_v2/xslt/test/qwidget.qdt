      <db:section>
         <db:title>void QWidget::closeEvent(QCloseEvent *event)</db:title>
         <db:methodsynopsis>
            <db:void/>
            <db:methodname>closeEvent</db:methodname>
            <db:methodparam>
               <db:type>QCloseEvent *</db:type>
               <db:parameter>event</db:parameter>
            </db:methodparam>
            <db:modifier>protected</db:modifier>
