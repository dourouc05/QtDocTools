                     <db:td>
                    void
                  </db:td>
                     <db:td>setAccessibleName(QStringaccessibleName)
                  </db:td>
                  </db:tr>
               </db:tbody>
            </db:informaltable>
         </db:para>
      </db:section>
      <db:section>
         <db:title>accessibleDescription : QString</db:title>
         <db:fieldsynopsis>
            <db:modifier>(Qt property)</db:modifier>
            <db:type>QString</db:type>
            <db:varname>accessibleDescription</db:varname>
         </db:fieldsynopsis>
         <db:methodsynopsis>
