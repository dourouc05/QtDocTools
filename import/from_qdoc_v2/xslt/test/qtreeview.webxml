        <class threadsafety="unspecified" name="QTreeView" href="qtreeview.html" status="active" access="public" location="itemviews\qtreeview.h" filepath="C:/Qt/5.11.1/Src/qtbase/src/widgets/itemviews\qtreeview.h" lineno="55" bases="QAbstractItemView" module="QtWidgets" groups="model-view,advanced" brief="Default model/view implementation of a tree view">
