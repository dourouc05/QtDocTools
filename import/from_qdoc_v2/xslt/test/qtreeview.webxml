            <property threadsafety="unspecified" name="indentation" fullname="QTreeView::indentation" href="qtreeview.html#indentation-prop" status="active" access="public" location="itemviews\qtreeview.h" filepath="C:/Qt/5.11.1/Src/qtbase/src/widgets/itemviews\qtreeview.h" lineno="59" type="int" brief="Of the items in the tree view">
